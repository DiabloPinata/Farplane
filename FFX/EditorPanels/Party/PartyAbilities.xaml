<UserControl x:Class="Farplane.FFX.EditorPanels.Party.PartyAbilities"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Farplane.FFX.EditorPanels.Party"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="400">
    <GroupBox Header="Abilities" Margin="0">
        <TabControl Name="TabAbilities">
            <TabItem Name="TabSkills" Header="Skills" />
            <TabItem Name="TabSpecial" Header="Special" />
            <TabItem Name="TabWhiteMagic" Header="White Magic" />
            <TabItem Name="TabBlackMagic" Header="Black Magic" />
        </TabControl>
    </GroupBox>
</UserControl>
