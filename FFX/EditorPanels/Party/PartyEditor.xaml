<UserControl x:Class="Farplane.FFX.EditorPanels.Party.PartyEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Farplane.FFX.EditorPanels.Party"
             xmlns:FFXEditor="clr-namespace:Farplane.FFX.EditorPanels"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="450">
    <TabControl Padding="0" Margin="0">
        <TabItem Header="Stats">
            <local:PartyStats x:Name="PartyStats" />
        </TabItem>
        <TabItem Header="Abilities">
            <local:PartyAbilities x:Name="PartyAbilities" />
        </TabItem>
        <TabItem Header="Overdrive">
            <local:PartyOverdrive x:Name="PartyOverdrive"  Margin="0,-2,0,0"/>
        </TabItem>
    </TabControl>
</UserControl>
