<UserControl x:Class="Farplane.FFX.EditorPanels.Party.PartyEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Farplane.FFX.EditorPanels.Party"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="450">
    <TabControl>
        <TabItem Header="Stats">
            <local:PartyStats x:Name="PartyStats"/>
        </TabItem>
        <TabItem Header="Abilities" />
        <TabItem Header="Overdrive" />
    </TabControl>
</UserControl>
