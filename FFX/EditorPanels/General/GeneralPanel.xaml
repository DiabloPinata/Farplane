<UserControl x:Class="Farplane.FFX.EditorPanels.General.GeneralPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Farplane.FFX.EditorPanels.General"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <GroupBox Header="General">
            <StackPanel>
            <DockPanel Margin="2">
                    <TextBlock Margin="5">Gil</TextBlock>
                    <TextBox Name="TextGil" Width="80" HorizontalAlignment="Right" KeyDown="TextGil_OnKeyDown"></TextBox>
                </DockPanel>
            <DockPanel Margin="2">
                <TextBlock Margin="5">Tidus Overdrive Times Used</TextBlock>
                <TextBox Name="TextTidusOverdrive" Width="80" HorizontalAlignment="Right" KeyDown="TextTidusOverdrive_OnKeyDown"></TextBox>
            </DockPanel>
        </StackPanel>
    </GroupBox>
</UserControl>
